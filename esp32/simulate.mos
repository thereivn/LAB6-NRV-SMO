// Clear any previous errors
clear();

// Load the model
loadFile("ESP32_CAM_Thermal_Energy.mo");
getErrorString();

// Check the model syntax
checkModel(ESP32_CAM_Thermal_Energy);
getErrorString();

// Simulate and write directly to mounted volume
simulate(ESP32_CAM_Thermal_Energy, stopTime=14400, outputFormat="csv", 
         numberOfIntervals=500, fileNamePrefix="/output/ESP32_CAM_Results");
getErrorString();